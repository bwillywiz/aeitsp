        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	if (isLoading || newValue == '') {
		return;
	}

	var unit_record = g_form.getReference('busniess_unit', getManager);
}

function getManager(unit){
	if (unit)
	{
		var gr = new GlideRecord('sys_user');
		gr.addQuery('name',unit.u_process_owner);
		gr.query();
		
		g_form.setValue('process_manager', '');
		if(gr.next())
		{
			g_form.setValue('process_manager', gr.sys_id);
		}
		
	}
}


